{
  "name": "github-user-list",
  "version": "0.1.0",
  "license": "MIT",
  "author": "physicsSorcererKing",
  "scripts": {
    "dev": "concurrently yarn:watch-* -c auto",
    "build": "relay-compiler --validate && tsc && vite build",
    "lint": "cross-env NODE_ENV=production eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "cross-env NODE_ENV=production eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "watch-dev": "vite",
    "watch-relay": "relay-compiler --watch"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "eslint",
    "prettier",
    "husky",
    "lint-staged",
    "template",
    "github-api",
    "relay",
    "graphql"
  ],
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.16.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-relay": "15.0.0",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-relay": "14.1.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react": "^4.0.4",
    "babel-plugin-relay": "15.0.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "3.0.2",
    "relay-compiler": "15.0.0",
    "sass": "^1.66.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
