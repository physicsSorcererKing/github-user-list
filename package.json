{
  "name": "github-user-list",
  "version": "1.1.0",
  "license": "MIT",
  "author": "physicsSorcererKing",
  "scripts": {
    "dev": "relay-compiler && vite",
    "watch": "concurrently yarn:watch-* -c auto",
    "build": "relay-compiler --validate && tsc && vite build",
    "lint": "cross-env NODE_ENV=production eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "cross-env NODE_ENV=production eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "relay": "relay-compiler",
    "watch-dev": "vite",
    "watch-relay": "relay-compiler --watch"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "eslint",
    "prettier",
    "husky",
    "lint-staged",
    "template",
    "github-api",
    "relay",
    "graphql"
  ],
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.16.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-relay": "^16.0.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-relay": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.1.0",
    "babel-plugin-relay": "^16.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "3.0.3",
    "relay-compiler": "^17.0.0",
    "sass": "^1.69.5",
    "typescript": "5.2.2",
    "vite": "^4.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
